{
  "name": "uno-game",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "15.x"
  },
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "SET \"NODE_ENV=development\" SET DEBUG=uno-game:* & nodemon ./bin/www",
    "db:create:migration": "npx sequelize migration:generate --name ",
    "db:migrate": "npx sequelize db:migrate",
    "db:rollback": "npx sequelize db:migrate:undo"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "~4.16.1",
    "hbs": "^4.1.2",
    "http-errors": "^1.6.3",
    "morgan": "~1.9.1",
    "pg-promise": "^10.11.1",
    "sequelize": "^6.9.0",
    "sequelize-cli": "^6.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
